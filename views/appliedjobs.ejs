                        <h3 id="elements"> Employer/job provider</h3>
                        <div class="row 200%">    
                                    
                            </div>
                            <div class="12u$ 12u$(medium)">

                                <% results.forEach(function(result){ %>

                                    <p>Job Title: <%= result.jobtitle %></p>
                                    <p>Job Type: <%= result.jobtype %></p>
                                    <p>Job Category: <%= result.jobcategory %></p>
                                    <p>Job Location: <%= result.location %></p>
                                    <p>Salary Range: <%= result.minsalary %> - <%= result.maxsalary %> </p>
                                    <hr>
                           
                                <% }); %>
                                
                          </div>
                        </div>
                        </div>            
            
                    </div>
