        <!-- One -->
            <section id="one" class="wrapper style2">
                <div class="inner">
                    <div class="box">
                    <div class="content">

                    <!-- Elements -->
                        <h2 id="elements">PROFESSIONAL DETAILS</h2>
                        <div class="row 200%">    
                                    
                            </div>
                            <div class="12u$ 12u$(medium)">

                            
                                    

                                <!-- Form -->
                                    <h4>Work Details</h4>

                                    <form method="POST" action="/users/<%= wo %>/work" >
                                        <div class="row uniform">

                                            <div class="12u 12u$(xsmall)">
                                                <input type="text" name="company" placeholder="Company" />
                                            </div>

                                            <div class="12u 12u$(xsmall)">
                                                <input type="text" name="job" placeholder="Job" />
                                            </div>

                                            <div class="6u 12u$(xsmall)">
                                                <input type="date" name="joiningdate" placeholder="Joining Date" />
                                            </div>

                                            <div class="6u 12u$(xsmall)">
                                                <input type="date" name="leavingdate" placeholder="Leaving Date" />
                                            </div>

                                            <div class="12u 12u$(xsmall)">
                                                <textarea name="description">Description</textarea>
                                            </div>
                                            
                                            <div class="row uniform">
                                            
                                            <div class="12u$(small)">
                                                <input type="submit" value="Submit" class="fit" style="margin-left: 20em" />
                                            </div>

                                            </div>
                                            
                                        </div>

                                    </form>

                                    

                                    <!-- <form method="post" action="#"> -->
                

                            </div>
                        </div>
                        </div>
                    </div>
                </div>
            </section>

